//Azure JSON Request Body

{
  "name":"Azure01",
  "type":"yesoreyeram-infinity-datasource",
  "url":"https://dev.azure.com/defragovuk",
  "access":"proxy",
  "basicAuth":true,"basicAuthUser": "Sathish.Ponnusamy2@cognizant.com",
  "secureJsonData": {
    "basicAuthPassword": "XXXXX"
  },
        "jsonData": {
            "allowedHosts": [
                "https://dev.azure.com/defragovuk"
            ],
            "auth_method": "basicAuth",
            "customHealthCheckEnabled": true,
            "customHealthCheckUrl": "https://dev.azure.com/defragovuk",
            "global_queries": [],
            "httpHeaderName1": "Authorization",
            "httpHeaderName2": "Content-Type",
            "oauthPassThru": false,
            "pathEncodedUrlsEnabled": true,
            "unsecuredQueryHandling": "allow"
        }
}
